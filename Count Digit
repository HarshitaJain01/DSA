// n=22074
#include <iostream>
using namespace std;
int main() {
    int n;
	cin>>n;
    int y=n;
    int count =0;
    while(n>0){
        int m = n%10;
        n = n/10;
        if(m!=0){
            if(n%m==0){
            count++;
            }
        }
            
    }
    cout<<count;
}

output:
2
