#include <bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int o=0;
    for(int i=0; i<=n; i++){
        int m=0;
        for(int j=1; j<=i; j++){
            if(i%j==0){
                m=m+j;
            }    
        }
        o=o+m;
    }
    cout<<o;
}

//n=4
//ans=15

