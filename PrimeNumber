#include <bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int count=0;
        for(int i=1; i<=n; i++){
            if(n%i==0){
                count++;
            }
        }
    cout<<count<<endl;
    if(count<=2){
        cout<<"It is Prime";
    }
    else{
        cout<<"Not Prime";
    }
}


